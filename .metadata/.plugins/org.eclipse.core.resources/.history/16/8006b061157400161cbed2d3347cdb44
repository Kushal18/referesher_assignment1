<!DOCTYPE html>
<html>
<body>

<h1> My Music Playlist </h1>
<input type = "text" id ="music" name="music"/>
<input type = "button" value="save" id="save" onclick = "save()"/>

</body>
<script>
function save(){
	var music = [];
	var musicCount = document.getElementsByName("music");
	for(var i = 0 ; i < musicCount ; i++ ){
		var songs = document.getElementsByName("music")[i].value;
		music.push(songs);
	}	
}

</script>
</html>


